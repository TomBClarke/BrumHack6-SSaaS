{
  "manifest_version": 2,

  "name": "SSaaS",
  "description": "Makes the Internet a safe-space",
  "version": "1.0",

  "browser_action": {
    "default_popup": "popup.html"
  },

  "permissions": [
    "storage"
  ],

  "content_scripts": [ {
    "js": [  "jquery.min.js", "background.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}
